/dts-v1/;

/ {
	#address-cells = <0x01>;
	#size-cells = <0x01>;

	chosen {
		bootargs = "earlycon=sbi debug console=ttyS0 memtest=0 root=/dev/mmcblk0p2 rootwait rootfstype=ext2 debug";
	};

	memory@80000000 {
		device_type = "memory";
		reg = <0x80000000 0x2000000>;
	};

	cpus {
		#address-cells = <0x01>;
		#size-cells = <0x00>;
		timebase-frequency = <0xf4240>;

		cpu@0 {
			phandle = <0x01>;
			device_type = "cpu";
			reg = <0x00>;
			status = "okay";
			compatible = "kianv\0riscv";
			riscv,isa = "rv32ima";
      riscv,isa-base = "rv32i";
      riscv,isa-extensions = "i", "m", "a", "sstc", "zicntr", "zicsr", "zifencei";
      mmu-type = "riscv,sv32";
			uarch = "KianV,sv32";

			interrupt-controller {
				#interrupt-cells = <0x01>;
				interrupt-controller;
				compatible = "riscv,cpu-intc";
				phandle = <0x02>;
			};
		};

		cpu-map {

			cluster0 {

				core0 {
					cpu = <0x01>;
				};
			};
		};
	};

	soc {
		#address-cells = <0x01>;
		#size-cells = <0x01>;
		compatible = "simple-bus";
		ranges;

		uart@10000000 {
			device_type = "serial";
			clock-frequency = <0x1000000>;
			reg = <0x10000000 0x100>;
			compatible = "ns16850";
			status = "okay";
		};

		uart@10000100 {
			device_type = "serial";
			clock-frequency = <0x1000000>;
			reg = <0x10000100 0x100>;
			compatible = "ns16850";
			status = "okay";
		};

		poweroff {
			value = <0x5555>;
			offset = <0x00>;
			regmap = <0x04>;
			compatible = "syscon-poweroff";
		};

		reboot {
			value = <0x7777>;
			offset = <0x00>;
			regmap = <0x04>;
			compatible = "syscon-reboot";
		};

		syscon@11100000 {
			phandle = <0x04>;
			reg = <0x11100000 0x1000>;
			compatible = "syscon";
		};

		clint@2000000 {
			interrupts-extended = <0x02 0x03 0x02 0x07>;
			reg = <0x2000000 0x10000>;
			compatible = "sifive,clint0\0riscv,clint0";
		};

		gpio@10000700 {
			compatible = "kianV,kianv-gpio";
			reg = <0x10000700 0x1000>;
			gpio-controller;
			#gpio-cells = <0x02>;
			status = "okay";
		};

		spi@10500000 {
			compatible = "kianVfpga,kianVspi-simple";
			reg = <0x10500000 0x100>;
			status = "okay";
			kianVfpga,num-cs = <0x01>;
			kianVfpga,sck-frequency = <0x00>;
			spi-cpha = <0x00>;
			spi-cpol = <0x00>;
			#address-cells = <0x01>;
			#size-cells = <0x00>;

			mmc-slot@0 {
				compatible = "mmc-spi-slot";
				spi-max-frequency = <0x16e3600>;
				voltage-ranges = <0xc80 0xd48>;
				reg = <0x00>;
				status = "okay";
				disable-wp;
				non-removable;
				no-sdio;
				no-mmc;
			};
		};
	};

	spi@10500100 {
		compatible = "kianVfpga,kianVspi-simple2";
		reg = <0x10500100 0x100>;
		status = "okay";
		kianVfpga,num-cs = <0x01>;
		kianVfpga,sck-frequency = <0x00>;
		spi-cpha = <0x00>;
		spi-cpol = <0x00>;
		#address-cells = <0x01>;
		#size-cells = <0x00>;

		encx24j600@0 {
			compatible = "microchip,encx24j600";
			#address-cells = <0x01>;
			#size-cells = <0x01>;
			reg = <0x00>;
			spi-max-frequency = <0xb71b00>;
			status = "disabled";
		};

		enc28j60@0 {
			compatible = "microchip,enc28j60";
			#address-cells = <0x01>;
			#size-cells = <0x01>;
			reg = <0x00>;
			spi-max-frequency = <0xb71b00>;
			status = "disabled";
		};

		w5500@0 {
			compatible = "wiznet,w5500";
			#address-cells = <0x01>;
			#size-cells = <0x01>;
			reg = <0x00>;
			spi-max-frequency = <0xb71b00>;
			status = "disabled";
		};
	};

	spi@10500200 {
		compatible = "kianVfpga,kianVspi-simple3";
		reg = <0x10500200 0x100>;
    kianVfpga,supports-16bit;
		status = "okay";
		kianVfpga,num-cs = <0x01>;
		kianVfpga,sck-frequency = <0x00>;
		spi-cpha = <0x00>;
		spi-cpol = <0x00>;
		#address-cells = <0x01>;
		#size-cells = <0x00>;

		display@0 {
			fps = <0x19>;
			compatible = "solomon,ssd1331";
			spi-max-frequency = <0x17d7840>;
			reg = <0x00>;
			regwidth = <0x08>;
			buswidth = <0x08>;
			status = "okay";
		};
	};
};
